{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.js"
    },
    {
      "src": "/api/bulk/(.*)",
      "dest": "/api/bulk"
    },
    {
      "src": "/api/single/(.*)",
      "dest": "/api/single"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}