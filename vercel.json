{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/bulk/(.*)",
      "dest": "/pages/api/bulk.js"
    },
    {
      "src": "/api/single/(.*)",
      "dest": "/pages/api/single.js"
    },
    {
      "src": "/api/init-db",
      "dest": "/pages/api/init-db.js"
    }
  ]
}